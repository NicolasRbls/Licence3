{
  "manifest_version": 3,
  "name": "Enregistreur d'Historique",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon128.png"
    }
  }
}
